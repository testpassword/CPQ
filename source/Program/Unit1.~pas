unit Unit1;

interface

uses
  Windows, Messages, SysUtils, Variants, Classes, Graphics, Controls, Forms,
  Dialogs, StdCtrls, Menus, jpeg, ExtCtrls;

type
  TMain = class(TForm)
    FB: TButton;
    FL: TLabel;
    MainMenu1: TMainMenu;
    N1: TMenuItem;
    N2: TMenuItem;
    N3: TMenuItem;
    Image1: TImage;
    N4: TMenuItem;
    N101: TMenuItem;
    ver: TMenuItem;
    N290920161: TMenuItem;
    Name: TLabel;
    N11: TMenuItem;
    N21: TMenuItem;
    N31: TMenuItem;
    N5: TMenuItem;
    N6: TMenuItem;
    N7: TMenuItem;
    procedure FBClick(Sender: TObject);
    procedure Button1Click(Sender: TObject);
    procedure N1Click(Sender: TObject);
    procedure N7Click(Sender: TObject);
    procedure FormCreate(Sender: TObject);
    procedure FormDestroy(Sender: TObject);
    procedure FBContextPopup(Sender: TObject; MousePos: TPoint;
      var Handled: Boolean);
  private
    { Private declarations }
  public
    { Public declarations }
  end;

var
  Main: TMain;
  TF:TextFile;
  s1,s2,s3,s4:string;
const
  g=9.8;

implementation

uses Unit2, Unit3, Unit4;

{$R *.dfm}

procedure TMain.FormCreate(Sender: TObject);
begin
assignfile(TF,'results.txt');
append(TF);
end;

procedure TMain.FormDestroy(Sender: TObject);
begin
closefile(TF);
end;

procedure TMain.FBClick(Sender: TObject);
begin
StrenghtForm.Show;
Main.Hide;
end;

procedure TMain.Button1Click(Sender: TObject);
begin
Form3.Show;
end;

procedure TMain.N1Click(Sender: TObject);
begin
Form3.Show;
end;

procedure TMain.N7Click(Sender: TObject);
begin
Settings.Show;
end;

procedure TMain.FBContextPopup(Sender: TObject; MousePos: TPoint;
  var Handled: Boolean);
begin
ShowMessage('     F - сила' +#13#10+ '      СИ - Н' +#13#10+ '   Векторная');
end;

end.
